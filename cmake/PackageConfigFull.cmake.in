# - Config file for the @PACKAGE_UPPER_NAME@ package
# It defines the following variables
#  @PACKAGE_UPPER_NAME@_INCLUDE_DIRS - include directories for @PACKAGE_NAME@
#  @PACKAGE_UPPER_NAME@_LIBRARIES    - libraries to link against
#  @PACKAGE_UPPER_NAME@_VERSION      - @PACKAGE_NAME@ version

# Our library dependencies (contains definitions for IMPORTED targets)
set(NGLIBS
    core
    framework
)

set(QT_VERSIONS
    4
    5
)

foreach(NGLIB ${NGLIBS})
    foreach(QT_VER ${QT_VERSIONS})
        if(WIN32)
            set(PACKAGE_CONFIG_PATH ${CMAKE_CURRENT_LIST_DIR})
        elseif(APPLE)
            set(PACKAGE_CONFIG_PATH ${CMAKE_CURRENT_LIST_DIR} PATH)
            set(PACKAGE_CONFIG_PATH ${PACKAGE_CONFIG_PATH}/Library/Frameworks/@PACKAGE_NAME@_${NGLIB}${QT_VER}/Resources/CMake)
        endif()

        string(TOUPPER ${NGLIB} UPPER_NGLIB)
        if(NOT TARGET @PACKAGE_NAME@_${NGLIB}${QT_VER})
            if(EXISTS "${PACKAGE_CONFIG_PATH}/@PACKAGE_UPPER_NAME@_${UPPER_NGLIB}${QT_VER}Targets.cmake")
                include("${PACKAGE_CONFIG_PATH}/@PACKAGE_UPPER_NAME@_${UPPER_NGLIB}${QT_VER}Targets.cmake")
            endif()
        endif()
        if(TARGET @PACKAGE_NAME@_${NGLIB}${QT_VER})
            get_property(@PACKAGE_NAME@_${NGLIB}${QT_VER}_INCLUDE_DIRS TARGET @PACKAGE_NAME@ PROPERTY INTERFACE_INCLUDE_DIRECTORIES)
            set(@PACKAGE_UPPER_NAME@_LIBRARIES ${@PACKAGE_UPPER_NAME@_LIBRARIES} @PACKAGE_NAME@_${NGLIB}${QT_VER})
            set(@PACKAGE_UPPER_NAME@_INCLUDE_DIRS ${@PACKAGE_UPPER_NAME@_INCLUDE_DIRS} ${@PACKAGE_NAME@_${NGLIB}${QT_VER}_INCLUDE_DIRS})
        endif()
    endforeach()
endforeach()

set(@PACKAGE_UPPER_NAME@_VERSION "@VERSION@")
set(@PACKAGE_UPPER_NAME@_FOUND TRUE)
